{
  "name": "martine",
  "version": "1.0.2",
  "main": "./dist/martine.js",
  "types": "./dist/martine.d.ts",
  "license": "MIT",
  "author": "Matt Begent",
  "repository": {
    "type": "git",
    "url": "git@github.com:mattbegent/martine.git"
  },
  "keywords": [
    "typescript",
    "utility",
    "martine"
  ],
  "devDependencies": {
    "@types/jsdom": "^2.0.29",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "dts-generator": "^2.1.0",
    "jasmine-core": "~2.5.2",
    "jsdom": "^10.1.0",
    "jsdomify": "^3.1.0",
    "mocha": "^3.1.2",
    "mocha-jsdom": "^1.1.0",
    "ts-node": "^1.7.0",
    "tslint": "^5.1.0",
    "typedoc": "^0.5.10",
    "typescript": "^2.3.2"
  },
  "scripts": {
    "start": "tsc -w",
    "pretest": "tsc -p tsconfig.test.json",
    "test": "mocha --reporter spec",
    "lint": "tslint -c tslint.json src/martine.ts",
    "docs": "typedoc --out docs/ src/martine.ts",
    "gentypes": "dts-generator --project ./ --out dist/martine.d.ts",
    "release": "npm run test && npm run lint && npm run gentypes && npm run docs"
  }
}
